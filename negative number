#include<stdio.h>
int main()
{
	int n,i,a[100];
	printf("enter the size of array :");
	scanf("%d",&n);
	printf("enter the elements of array");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("negative number in this array :");
	for(i=0;i<n;i++)
	{
		if(a[i]<0)
		{
			printf("%d",a[i]);
		}
	}
	return 0;
}
